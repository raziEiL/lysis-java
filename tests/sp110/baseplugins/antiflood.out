public bool:OnClientFloodCheck(client)
{
	max_chat = ConVar.FloatValue.get(sm_flood_time);
	new var1;
	if (max_chat <= 0.0 || CheckCommandAccess(client, "sm_flood_access", 16384, true))
	{
		return 0;
	}
	if (playerinfo[client] >= GetGameTime())
	{
		if (playerinfo[client][1] >= 3)
		{
			return 1;
		}
	}
	return 0;
}

public void:OnClientFloodResult(client, bool:blocked)
{
	new var1;
	if (max_chat <= 0.0 || CheckCommandAccess(client, "sm_flood_access", 16384, true))
	{
		return 0;
	}
	new Float:curTime = GetGameTime();
	new Float:newTime = curTime + max_chat;
	if (playerinfo[client] >= curTime)
	{
		if (blocked)
		{
			newTime += 3.0;
		}
		else
		{
			if (playerinfo[client][1] < 3)
			{
				playerinfo[client][1]++;
			}
		}
	}
	else
	{
		if (0 < playerinfo[client][1])
		{
			playerinfo[client][1]--;
		}
	}
	playerinfo[client] = newTime;
	return 0;
}

public void:OnClientPutInServer(client)
{
	playerinfo[client] = 0;
	playerinfo[client][1] = 0;
	return 0;
}

